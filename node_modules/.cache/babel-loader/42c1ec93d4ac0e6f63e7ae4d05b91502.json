\">\r\n\t\t\t\t\t\t\t</mu-radio>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div>\r\n\t\t\t\t\t\t\t<mu-button slot=\"right\" color=\"primary\" @click=\"submit\">搜索</mu-button>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</mu-col>\r\n\t\t\t\t<mu-col span=\"12\" sm=\"12\">\r\n\t\t\t\t\t<mu-list textline=\"two-line\" @change=\"playMusic\">\r\n\t\t\t\t\t\t<mu-sub-header>歌曲列表</mu-sub-header>\r\n\t\t\t\t\t\t<mu-list-item avatar button :ripple=\"false\" :key=\"songItem.id\"\r\n\t\t\t\t\t\t\tv-for=\"(songItem,index) in songArr\" :value=\"songItem\">\r\n\t\t\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t\t\t<mu-avatar>\r\n\t\t\t\t\t\t\t\t\t<img :src=\"songItem.pic\">\r\n\t\t\t\t\t\t\t\t</mu-avatar>\r\n\t\t\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t\t\t<mu-list-item-content>\r\n\t\t\t\t\t\t\t\t<mu-list-item-title>{{songItem.name}}</mu-list-item-title>\r\n\t\t\t\t\t\t\t\t<mu-list-item-sub-title>{{songItem.singer}}</mu-list-item-sub-title>\r\n\t\t\t\t\t\t\t</mu-list-item-content>\r\n\t\t\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t\t\t<mu-button icon @click.stop=\"clickDownMusic(songItem)\">\r\n\t\t\t\t\t\t\t\t\t<mu-icon value=\"file_download\"></mu-icon>\r\n\t\t\t\t\t\t\t\t</mu-button>\r\n\t\t\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t\t</mu-list-item>\r\n\t\t\t\t\t</mu-list>\r\n\t\t\t\t</mu-col>\r\n\t\t\t\t<!-- \t\t\t\t<mu-col span=\"12\" xl=\"12\">\r\n\t\t\t\t</mu-col> -->\r\n\t\t\t</mu-row>\r\n\t\t</mu-container>\r\n\r\n\t\t<!-- 播放器 -->\r\n\t\t<div style=\"height: 120px;\">\r\n\t\t\t<div class=\"play_bottom\">\r\n\t\t\t\t<mu-card>\r\n\t\t\t\t\t<mu-card-text>\r\n\t\t\t\t\t\t<div id=\"aplayer\"></div>\r\n\t\t\t\t\t</mu-card-text>\r\n\t\t\t\t</mu-card>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\r\n\t\t<mu-bottom-sheet :open.sync=\"sheet_open\">\r\n\t\t\t<mu-list @item-click=\"closeBottomSheet\">\r\n\t\t\t\t<mu-sub-header>下载菜单</mu-sub-header>\r\n\r\n\t\t\t\t<mu-list-item button :key=\"sheet.id\" v-for=\"(sheet,index) in sheet_arr\" :value=\"sheet\">\r\n\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t<mu-icon value=\"file_download\" color=\"orange\"></mu-icon>\r\n\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t<mu-list-item-title>下载 {{sheet.suffix}}格式 {{sheet.kbps}} kb质量</mu-list-item-title>\r\n\t\t\t\t</mu-list-item>\r\n\r\n\r\n\t\t\t</mu-list>\r\n\t\t</mu-bottom-sheet>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\t// import HelloWorld from './components/HelloWorld.vue'\r\n\timport theme from 'muse-ui/lib/theme';\r\n\timport 'aplayer/dist/APlayer.min.css';\r\n\timport APlayer from 'aplayer';\r\n\t//用的地方调用：\r\n\timport {\r\n\t\tgetSearch, //歌曲搜索\r\n\t\tgetSong, //歌曲\r\n\t\tgetLyric //歌词\r\n\t} from '@/api';\r\n\texport default {\r\n\t\tname: 'app',\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tsheet_open: true,\r\n\t\t\t\tsheet_arr: [],\r\n\t\t\t\tdocked: false,\r\n\t\t\t\topen: false,\r\n\t\t\t\tposition: 'left',\r\n\t\t\t\t//播放器\r\n\t\t\t\t// autoplay: false,\r\n\t\t\t\tplay: {\r\n\t\t\t\t\tname: \"\", //歌曲名称\r\n\t\t\t\t\tartist: \"\", //歌曲这种描述\r\n\t\t\t\t\turl: \"\", //歌曲地址\r\n\t\t\t\t\tcover: \"\", //歌曲图片\r\n\t\t\t\t\tlrc: \"\" //歌词\r\n\t\t\t\t},\r\n\t\t\t\t//参数选择\r\n\t\t\t\tradio: \"1\",\r\n\t\t\t\tradioArr: [{\r\n\t\t\t\t\tid: \"1\",\r\n\t\t\t\t\tname: \"小鹅\"\r\n\t\t\t\t}, {\r\n\t\t\t\t\tid: \"2\",\r\n\t\t\t\t\tname: \"小咕\"\r\n\t\t\t\t}],\r\n\t\t\t\t//查询参数\r\n\t\t\t\tsearchParam: {\r\n\t\t\t\t\ttext: \"周杰伦\",\r\n\t\t\t\t\tpageNo: 1,\r\n\t\t\t\t\tpageSize: 30\r\n\t\t\t\t},\r\n\t\t\t\t//播放地址查询\r\n\t\t\t\tsongParam: {\r\n\t\t\t\t\tid: '',\r\n\t\t\t\t\tcode: ''\r\n\t\t\t\t},\r\n\t\t\t\t//歌词地址查询\r\n\t\t\t\tlyricParam: {\r\n\t\t\t\t\tid: ''\r\n\t\t\t\t},\r\n\t\t\t\ttheme_bool: true,\r\n\t\t\t\tsongArr: [],\r\n\t\t\t\t// ap: null\r\n\t\t\t}\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t\t// HelloWorld\r\n\t\t\t// Aplayer\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\t// Aplayer.disableVersionBadge = true;\r\n\r\n\t\t\tnew APlayer({\r\n\t\t\t\tcontainer: document.getElementById('aplayer')\r\n\t\t\t});\r\n\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t//更新日志\r\n\t\t\tupdateLog() {\r\n\t\t\t\tthis.$alert('0.1版本发布', '更新日志');\r\n\t\t\t},\r\n\t\t\t//关于\r\n\t\t\tabout() {\r\n\t\t\t\tthis.$alert('vip系统 - 0.1 Code by Vue and muse-ui', '关于');\r\n\t\t\t},\r\n\t\t\t//切换颜色\r\n\t\t\ttheme_toggle() {\r\n\r\n\t\t\t\tthis.theme_bool = !this.theme_bool\r\n\t\t\t\tif (this.theme_bool) {\r\n\t\t\t\t\ttheme.use('light');\r\n\t\t\t\t} else {\r\n\t\t\t\t\ttheme.use('dark');\r\n\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tsubmit() {\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\tthis.searchParam.type = ths.radio;\r\n\t\t\t\tgetSearch(this.searchParam).then(res => {\r\n\t\t\t\t\tconsole.log(res.data.data.items);\r\n\t\t\t\t\tths.songArr = res.data.data.items;\r\n\r\n\t\t\t\t})\r\n\r\n\t\t\t},\r\n\t\t\tclickDownMusic(item) {\r\n\t\t\t\t// alert(1);\r\n\t\t\t\tthis.sheet_open = true;\r\n\t\t\t\tthis.sheet_arr = item.kbpsList;\r\n\t\t\t\tconsole.log(item.kbpsList);\r\n\t\t\t},\r\n\t\t\tplayMusic(item) {\r\n\t\t\t\t// console.log(111);\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\tths.play.artist = item.singer;\r\n\t\t\t\tths.play.cover = item.pic;\r\n\t\t\t\tths.play.name = item.name;\r\n\r\n\t\t\t\t//获取播放\r\n\t\t\t\tths.songParam.id = item.id;\r\n\t\t\t\tths.songParam.code = 'size_128mp3';\r\n\t\t\t\tths.songParam.type = ths.radio;\r\n\t\t\t\tgetSong(ths.songParam).then(res => {\r\n\t\t\t\t\tconsole.log(res.data.data.url);\r\n\t\t\t\t\tths.play.url = res.data.data.url;\r\n\r\n\t\t\t\t\t//获取歌词\r\n\t\t\t\t\tths.lyricParam.id = item.id;\r\n\t\t\t\t\tths.lyricParam.type = ths.radio;\r\n\t\t\t\t\tgetLyric(ths.lyricParam).then(res => {\r\n\t\t\t\t\t\t// console.log(res.data.data);\r\n\t\t\t\t\t\tconsole.log(res.data.data);\r\n\t\t\t\t\t\tths.play.lrc = res.data.data;\r\n\t\t\t\t\t\tths.play.type = ths.radio;\r\n\t\t\t\t\t\tconst ap = new APlayer({\r\n\t\t\t\t\t\t\t// autoplay: true,\r\n\t\t\t\t\t\t\tpreload: 'metadata',\r\n\t\t\t\t\t\t\tlrcType: 1,\r\n\t\t\t\t\t\t\tcontainer: document.getElementById('aplayer'),\r\n\t\t\t\t\t\t\taudio: [ths.play]\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t\tap.play();\r\n\t\t\t\t\t})\r\n\r\n\t\t\t\t})\r\n\r\n\r\n\r\n\r\n\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.search-plane {\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: space-between;\r\n\t}\r\n\r\n\t.search {\r\n\t\tmargin-top: 3vh;\r\n\t}\r\n\r\n\t.play_bottom {\r\n\t\tposition: fixed;\r\n\t\tbottom: 0;\r\n\t\t/* border: 1px solid red; */\r\n\t\twidth: 100%;\r\n\t\tleft: 0;\r\n\t\tright: 0;\r\n\t\tbottom: 0;\r\n\t\tz-index: 101;\r\n\t}\r\n\r\n\t/* #app {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n} */\r\n\t* {\r\n\t\tpadding: 0;\r\n\t\tmargin: 0;\r\n\t}\r\n\r\n\thtml,\r\n\tbody,\r\n\t#app {\r\n\t\theight: 100%;\r\n\r\n\t}\r\n\r\n\t.mu-appbar-header {\r\n\t\tposition: fixed;\r\n\t\tleft: 0;\r\n\t\tright: 0;\r\n\t\ttop: 0;\r\n\t\tz-index: 101;\r\n\t\toverflow: hidden;\r\n\t}\r\n</style>\n"],"sourceRoot":"src"}]}