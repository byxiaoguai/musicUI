data.data.url);\r\n\t\t\t\t\tths.play.url = res.data.data.url;\r\n\r\n\t\t\t\t\t//获取歌词\r\n\t\t\t\t\tths.lyricParam.id = item.id;\r\n\t\t\t\t\tths.lyricParam.type = ths.radio;\r\n\t\t\t\t\tgetLyric(ths.lyricParam).then(res => {\r\n\t\t\t\t\t\t// console.log(res.data.data);\r\n\t\t\t\t\t\t// console.log(res.data.data);\r\n\t\t\t\t\t\tths.play.lrc = res.data.data;\r\n\t\t\t\t\t\tths.play.type = ths.radio;\r\n\t\t\t\t\t\tconst ap = new APlayer({\r\n\t\t\t\t\t\t\tautoplay: true,\r\n\t\t\t\t\t\t\tpreload: 'metadata',\r\n\t\t\t\t\t\t\tlrcType: 1,\r\n\t\t\t\t\t\t\tcontainer: document.getElementById('aplayer'),\r\n\t\t\t\t\t\t\taudio: [ths.play]\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t\tap.pause();\r\n\t\t\t\t\t\tap.play();\r\n\t\t\t\t\t})\r\n\r\n\t\t\t\t})\r\n\r\n\r\n\r\n\r\n\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.search-plane {\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: space-between;\r\n\t}\r\n\r\n\t.search {\r\n\t\tmargin-top: 3vh;\r\n\t}\r\n\r\n\t.play_bottom {\r\n\t\tposition: fixed;\r\n\t\tbottom: 0;\r\n\t\t/* border: 1px solid red; */\r\n\t\twidth: 100%;\r\n\t\tleft: 0;\r\n\t\tright: 0;\r\n\t\tbottom: 0;\r\n\t\tz-index: 101;\r\n\t}\r\n\r\n\t/* #app {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n} */\r\n\t* {\r\n\t\tpadding: 0;\r\n\t\tmargin: 0;\r\n\t}\r\n\r\n\thtml,\r\n\tbody,\r\n\t#app {\r\n\t\theight: 100%;\r\n\r\n\t}\r\n\r\n\t.mu-appbar-header {\r\n\t\tposition: fixed;\r\n\t\tleft: 0;\r\n\t\tright: 0;\r\n\t\ttop: 0;\r\n\t\tz-index: 101;\r\n\t\toverflow: hidden;\r\n\t}\r\n</style>\n"]}]}