con :value=\"theme_bool?'wb_sunny':'brightness_4'\"></mu-icon>\r\n\t\t\t</mu-button> -->\r\n\t\t</mu-appbar>\r\n\r\n\t\t<!-- 内容 -->\r\n\t\t<mu-container style=\"margin-top: 60px;\">\r\n\t\t\t<mu-row gutter>\r\n\t\t\t\t<mu-col span=\"12\">\r\n\t\t\t\t\t<div class=\"search\">\r\n\t\t\t\t\t\t<mu-text-field full-width v-model=\"searchParam.text\" placeholder=\"请输入歌手歌曲关键词\">\r\n\t\t\t\t\t\t</mu-text-field>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</mu-col>\r\n\t\t\t\t<mu-col span=\"12\">\r\n\t\t\t\t\t<div class=\"search-plane\">\r\n\t\t\t\t\t\t<div>\r\n\t\t\t\t\t\t\t<mu-radio :key=\"radioItem.id\" v-for=\"(radioItem,index) in radioArr\" :value=\"radioItem.id\"\r\n\t\t\t\t\t\t\t\tv-model=\"radio\" :label=\"radioItem.name\">\r\n\t\t\t\t\t\t\t</mu-radio>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div>\r\n\t\t\t\t\t\t\t<mu-button slot=\"right\" color=\"primary\" @click=\"submit\">搜索</mu-button>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</mu-col>\r\n\t\t\t\t<mu-col span=\"12\" sm=\"12\">\r\n\t\t\t\t\t<mu-list textline=\"two-line\" @change=\"playMusic\">\r\n\t\t\t\t\t\t<mu-sub-header>歌曲列表</mu-sub-header>\r\n\t\t\t\t\t\t<mu-list-item avatar button :ripple=\"false\" :key=\"songItem.id\"\r\n\t\t\t\t\t\t\tv-for=\"(songItem,index) in songArr\" :value=\"songItem\">\r\n\t\t\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t\t\t<mu-avatar>\r\n\t\t\t\t\t\t\t\t\t<img :src=\"songItem.pic\">\r\n\t\t\t\t\t\t\t\t</mu-avatar>\r\n\t\t\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t\t\t<mu-list-item-content>\r\n\t\t\t\t\t\t\t\t<mu-list-item-title>{{songItem.name}}</mu-list-item-title>\r\n\t\t\t\t\t\t\t\t<mu-list-item-sub-title>{{songItem.singer}}</mu-list-item-sub-title>\r\n\t\t\t\t\t\t\t</mu-list-item-content>\r\n\t\t\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t\t\t<mu-button icon @click.stop=\"clickDownMusic(songItem)\">\r\n\t\t\t\t\t\t\t\t\t<mu-icon value=\"file_download\"></mu-icon>\r\n\t\t\t\t\t\t\t\t</mu-button>\r\n\t\t\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t\t</mu-list-item>\r\n\t\t\t\t\t</mu-list>\r\n\t\t\t\t</mu-col>\r\n\t\t\t\t<!-- \t\t\t\t<mu-col span=\"12\" xl=\"12\">\r\n\t\t\t\t</mu-col> -->\r\n\t\t\t</mu-row>\r\n\t\t</mu-container>\r\n\r\n\t\t<!-- 播放器 -->\r\n\t\t<div style=\"height: 120px;\">\r\n\t\t\t<div class=\"play_bottom\">\r\n\t\t\t\t<mu-card>\r\n\t\t\t\t\t<mu-card-text>\r\n\t\t\t\t\t\t<div id=\"aplayer\"></div>\r\n\t\t\t\t\t</mu-card-text>\r\n\t\t\t\t</mu-card>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n         <!-- 下载 -->\r\n\t\t<mu-bottom-sheet :open.sync=\"sheet_open\" v-loading=\"loading\">\r\n\t\t\t<mu-list @item-click=\"closeBottomSheet\">\r\n\t\t\t\t<mu-sub-header>下载菜单</mu-sub-header>\r\n\r\n\t\t\t\t<mu-list-item button v-if=\"sheet_flac!=null\" value=\"size_flac\">\r\n\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t<mu-icon value=\"file_download\" color=\"orange\"></mu-icon>\r\n\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t<mu-list-item-title>下载无损质量 <span class=\"size_music size_sq\">SQ</span></mu-list-item-title>\r\n\t\t\t\t</mu-list-item>\r\n\r\n\r\n\t\t\t\t<mu-list-item button v-if=\"sheet_flac!=null\" value=\"size_320mp3\">\r\n\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t<mu-icon value=\"file_download\" color=\"blue\"></mu-icon>\r\n\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t<mu-list-item-title>下载高品质质量 <span class=\"size_music size_hq\">HQ</span></mu-list-item-title>\r\n\t\t\t\t</mu-list-item>\r\n\r\n\t\t\t\t<mu-list-item button v-if=\"sheet_flac!=null\" value=\"size_128mp3\">\r\n\t\t\t\t\t<mu-list-item-action>\r\n\t\t\t\t\t\t<mu-icon value=\"file_download\" color=\"green\"></mu-icon>\r\n\t\t\t\t\t</mu-list-item-action>\r\n\t\t\t\t\t<mu-list-item-title>下载普通音质</mu-list-item-title>\r\n\t\t\t\t</mu-list-item>\r\n\r\n\r\n\t\t\t</mu-list>\r\n\t\t</mu-bottom-sheet>\r\n\t\r\n\t    <!-- 下载 -->\r\n\t  <mu-dialog title=\"Dialog\" width=\"360\" :open.sync=\"openSimple\">\r\n\t   获取地址成功，点击下面按钮即可下载\r\n\t   由于系统限制，请自行重命名\r\n\t   \r\n\t   如果地址打开不对，请多解析几次看看\r\n\t    <mu-button slot=\"actions\" flat color=\"primary\" @click=\"copyName\">复制名称</mu-button>\r\n\t \t    <mu-button slot=\"actions\" flat color=\"primary\" @click=\"downUrl\">点击下载</mu-button>\r\n\t  </mu-dialog>\r\n\t\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\t// import HelloWorld from './components/HelloWorld.vue'\r\n\timport theme from 'muse-ui/lib/theme';\r\n\timport 'aplayer/dist/APlayer.min.css';\r\n\timport APlayer from 'aplayer';\r\n\t//用的地方调用：\r\n\timport {\r\n\t\tgetSearch, //歌曲搜索\r\n\t\tgetSong, //歌曲\r\n\t\tgetLyric //歌词\r\n\t} from '@/api';\r\n\texport default {\r\n\t\tname: 'app',\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\t openSimple: false,//下载信息弹窗\r\n\t\t\t\tsheet_open: false, //下载弹窗\r\n\t\t\t\tsheet_flac: null, //下载无损\r\n\t\t\t\tsheet_gzl: null, //下载高质量\r\n\t\t\t\tsheet_pt: null, //下载普通音质\r\n\t\t\t\tdocked: false,\r\n\t\t\t\topen: false,\r\n\t\t\t\tposition: 'left',\r\n\t\t\t\t//播放器\r\n\t\t\t\t// autoplay: false,\r\n\t\t\t\tplay: {\r\n\t\t\t\t\tname: \"\", //歌曲名称\r\n\t\t\t\t\tartist: \"\", //歌曲这种描述\r\n\t\t\t\t\turl: \"\", //歌曲地址\r\n\t\t\t\t\tcover: \"\", //歌曲图片\r\n\t\t\t\t\tlrc: \"\" //歌词\r\n\t\t\t\t},\r\n\t\t\t\t//参数选择\r\n\t\t\t\tradio: \"1\",\r\n\t\t\t\tradioArr: [{\r\n\t\t\t\t\tid: \"1\",\r\n\t\t\t\t\tname: \"小鹅\"\r\n\t\t\t\t}, {\r\n\t\t\t\t\tid: \"2\",\r\n\t\t\t\t\tname: \"小咕\"\r\n\t\t\t\t}],\r\n\t\t\t\t//查询参数\r\n\t\t\t\tsearchParam: {\r\n\t\t\t\t\ttext: \"周杰伦\",\r\n\t\t\t\t\tpageNo: 1,\r\n\t\t\t\t\tpageSize: 30\r\n\t\t\t\t},\r\n\t\t\t\t//播放地址查询\r\n\t\t\t\tsongParam: {\r\n\t\t\t\t\tid: '',\r\n\t\t\t\t\tcode: ''\r\n\t\t\t\t},\r\n\t\t\t\t//歌词地址查询\r\n\t\t\t\tlyricParam: {\r\n\t\t\t\t\tid: ''\r\n\t\t\t\t},\r\n\t\t\t\ttheme_bool: true,\r\n\t\t\t\tsongArr: [],\r\n\t\t\t\tloading: false\r\n\t\t\t\t// ap: null\r\n\t\t\t}\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t\t// HelloWorld\r\n\t\t\t// Aplayer\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\t// Aplayer.disableVersionBadge = true;\r\n\r\n\t\t\tnew APlayer({\r\n\t\t\t\tcontainer: document.getElementById('aplayer')\r\n\t\t\t});\r\n\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t//下载url\r\n\t\t\tdownUrl(){\r\n\t\t\t\t\r\n\t\t\t},\r\n\t\t\t//复制名称\r\n\t\t\tcopyName(){\r\n\t\t\t\t\r\n\t\t\t},\r\n\t\t\t//下载底部弹窗\r\n\t\t\tcloseBottomSheet(item) {\r\n\t\t\t\tlet ths = this;\r\n\r\n\t\t\t\tths.songParam.code = item.value;\r\n\t\t\t\tgetSong(ths.songParam).then(res => {\r\n\t\t\t\t\tconsole.log(res.data.data.url);\r\n\t\t\t\t\t// ths.play.url = res.data.data.url;\r\n\t\t\t\t ths.openSimple = true;\r\n\t\t\t\t\r\n\t\t\t\t})\r\n\t\t\t\t\r\n\t\t\t\tthis.loading = true;\r\n\t\t\t\tsetTimeout(() => {\r\n\t\t\t\t\tthis.loading = false;\r\n\t\t\t\t}, 3000)\r\n\t\t\t},\r\n\r\n\t\t\t//更新日志\r\n\t\t\tupdateLog() {\r\n\t\t\t\tthis.$alert('0.1版本发布', '更新日志');\r\n\t\t\t},\r\n\t\t\t//关于\r\n\t\t\tabout() {\r\n\t\t\t\tthis.$alert('vip系统 - 0.1 Code by Vue and muse-ui', '关于');\r\n\t\t\t},\r\n\t\t\t//切换颜色\r\n\t\t\ttheme_toggle() {\r\n\r\n\t\t\t\tthis.theme_bool = !this.theme_bool\r\n\t\t\t\tif (this.theme_bool) {\r\n\t\t\t\t\ttheme.use('light');\r\n\t\t\t\t} else {\r\n\t\t\t\t\ttheme.use('dark');\r\n\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tsubmit() {\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\tthis.searchParam.type = ths.radio;\r\n\t\t\t\tgetSearch(this.searchParam).then(res => {\r\n\t\t\t\t\t// console.log(res.data.data.items);\r\n\t\t\t\t\tths.songArr = res.data.data.items;\r\n\r\n\t\t\t\t})\r\n\r\n\t\t\t},\r\n\t\t\tclickDownMusic(item) {\r\n\t\t\t\tthis.sheet_open = true;\r\n\t\t\t\tthis.sheet_flac = item.kbpsList.filter(item => item.code == 'size_flac')[0];\r\n\t\t\t\tthis.sheet_gzl = item.kbpsList.filter(item => item.code == 'size_320mp3')\r\n\t\t\t\tthis.sheet_pt = item.kbpsList.filter(item => item.code == 'size_128mp3')\r\n\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\t//获取播放\r\n\t\t\t\tths.songParam.id = item.id;\r\n\t\t\t\tths.songParam.type = ths.radio;\r\n\t\t\t},\r\n\t\t\tplayMusic(item) {\r\n\t\t\t\t// console.log(111);\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\tths.play.artist = item.singer;\r\n\t\t\t\tths.play.cover = item.pic;\r\n\t\t\t\tths.play.name = item.name;\r\n\r\n\t\t\t\t//获取播放\r\n\t\t\t\tths.songParam.id = item.id;\r\n\t\t\t\tths.songParam.code = 'size_128mp3';\r\n\t\t\t\tths.songParam.type = ths.radio;\r\n\t\t\t\tgetSong(ths.songParam).then(res => {\r\n\t\t\t\t\t// console.log(res.data.data.url);\r\n\t\t\t\t\tths.play.url = res.data.data.url;\r\n\r\n\t\t\t\t\t//获取歌词\r\n\t\t\t\t\tths.lyricParam.id = item.id;\r\n\t\t\t\t\tths.lyricParam.type = ths.radio;\r\n\t\t\t\t\tgetLyric(ths.lyricParam).then(res => {\r\n\t\t\t\t\t\t// console.log(res.data.data);\r\n\t\t\t\t\t\t// console.log(res.data.data);\r\n\t\t\t\t\t\tths.play.lrc = res.data.data;\r\n\t\t\t\t\t\tths.play.type = ths.radio;\r\n\t\t\t\t\t\tconst ap = new APlayer({\r\n\t\t\t\t\t\t\tautoplay: true,\r\n\t\t\t\t\t\t\tpreload: 'metadata',\r\n\t\t\t\t\t\t\tlrcType: 1,\r\n\t\t\t\t\t\t\tcontainer: document.getElementById('aplayer'),\r\n\t\t\t\t\t\t\taudio: [ths.play]\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t\t// ap.pause();\r\n\t\t\t\t\t\tap.play();\r\n\t\t\t\t\t})\r\n\r\n\t\t\t\t})\r\n\r\n\r\n\r\n\r\n\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.search-plane {\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: space-between;\r\n\t}\r\n\r\n\t.search {\r\n\t\tmargin-top: 3vh;\r\n\t}\r\n\r\n\t.play_bottom {\r\n\t\tposition: fixed;\r\n\t\tbottom: 0;\r\n\t\t/* border: 1px solid red; */\r\n\t\twidth: 100%;\r\n\t\tleft: 0;\r\n\t\tright: 0;\r\n\t\tbottom: 0;\r\n\t\tz-index: 101;\r\n\t}\r\n\r\n\t/* #app {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n} */\r\n\t* {\r\n\t\tpadding: 0;\r\n\t\tmargin: 0;\r\n\t}\r\n\r\n\thtml,\r\n\tbody,\r\n\t#app {\r\n\t\theight: 100%;\r\n\r\n\t}\r\n\r\n\t.mu-appbar-header {\r\n\t\tposition: fixed;\r\n\t\tleft: 0;\r\n\t\tright: 0;\r\n\t\ttop: 0;\r\n\t\tz-index: 101;\r\n\t\toverflow: hidden;\r\n\t}\r\n</style>\n"]}]}