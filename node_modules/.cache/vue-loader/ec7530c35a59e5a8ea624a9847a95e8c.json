r\n\t\tmounted() {\r\n\t\t\tAplayer.disableVersionBadge = true;\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t//切换颜色\r\n\t\t\ttheme_toggle() {\r\n\r\n\t\t\t\tthis.theme_bool = !this.theme_bool\r\n\t\t\t\tif (this.theme_bool) {\r\n\t\t\t\t\ttheme.use('light');\r\n\t\t\t\t} else {\r\n\t\t\t\t\ttheme.use('dark');\r\n\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tsubmit() {\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\tgetSearch(this.searchParam).then(res => {\r\n\t\t\t\t\tconsole.log(res.data.data.items);\r\n\t\t\t\t\tths.songArr = res.data.data.items;\r\n\r\n\t\t\t\t})\r\n\t\t\t\t// this.$refs.form.validate().then((result) => {\r\n\t\t\t\t// \tconsole.log('form valid: ', result)\r\n\t\t\t\t// });\r\n\r\n\t\t\t},\r\n\t\t\tclickDownMusic(item) {\r\n\t\t\t\talert(1);\r\n\r\n\t\t\t},\r\n\t\t\tplayMusic(item){\r\n\t\t\t\tconsole.log(111);\r\n\t\t\t\tlet ths = this;\r\n\t\t\t\tths.play.artist=item.singer;\r\n\t\t\t\tths.play.pic=item.pic;\r\n\t\t\t\tths.play.title=item.name;\r\n\t\t\t\r\n\t\t\t    //获取播放\r\n\t\t\t\tths.songParam.id=item.id;\r\n\t\t\t\tths.songParam.code=item.kbpsList[0].code;\r\n\t\t\t\tgetQQSong(ths.songParam).then(res => {\r\n\t\t\t\t\tconsole.log(res.data.data.url);\r\n\t\t\t\t\tths.play.src = res.data.data.url;\r\n\t\t\t\t\tths.autoplay=true;\r\n\t\t\t\t})\r\n\t\t\t\t//获取歌词\r\n\t\t\t\t// ths.lyricParam.id=item.id;\r\n\t\t\t\t// getLyric(ths.lyricParam).then(res => {\r\n\t\t\t\t// \tths.play.lrc = res.data.data;\r\n\t\t\t\t// })\r\n\t\t\t\t\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.search-plane {\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: space-between;\r\n\t}\r\n\r\n\t.search {\r\n\t\tmargin-top: 3vh;\r\n\t}\r\n\r\n\t.play_bottom {\r\n\t\tposition: absolute;\r\n\t\tbottom: 0;\r\n\t\t/* border: 1px solid red; */\r\n\t\twidth: 100%;\r\n\t}\r\n\r\n\t/* #app {\r\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60p